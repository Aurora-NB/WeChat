
page{
  height: 90%;
}
.individuals{
  background: #23EBB9;
  padding-top: 50rpx;
  margin: 0;
  width: 100%;
  height: 500rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.user {
  /* background-color: #ffffff; */

  width: 90%;
  height: 100px;
  border-radius: 5px;
  display: flex;
  align-items: center;
  padding-left: 10rpx;

}

.userhead {
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  background-color: rgb(187, 186, 186);
  /* background-color:#ffffff; */
  border: 1px solid black;
  margin-right: 30rpx;
}

.detail {

  height: 60%;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.other-options {
  background-color: #ffffff;
  margin-top: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  padding-right: 40rpx;
}

.options {
  background-color: #ffffff;
  width: 90%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding-right:50rpx;
  padding-top:20rpx ;
  padding-bottom: 20rpx;
  padding-left: 50rpx;
  border: none;
}

.arrow {
  height: 30rpx;
  width: 30rpx;
}

.button-log {
  height: 7vw;
  font-size: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 0;
  margin: 0;
}
 
.view-log{
  display: flex;
  width: 100%;
  height: 7vw;
}

button::after {
  margin: 0;
  border: none;
}

.gif-wave {
  background-color: #23EBB9;
  width: 800rpx;
  z-index: 99;
  position: relative;
  mix-blend-mode: screen;
  height: 17vw;
  bottom:39vw;
}